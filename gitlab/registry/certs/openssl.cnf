[req]
default_bits = 4096
prompt = no
default_md = sha256
x509_extensions = v3_req
distinguished_name = dn

[dn]
C = US
ST = State
L = City
O = Organization
CN = registry.meysamaghili.ir

[v3_req]
subjectAltName = @alt_names

[alt_names]
DNS.1 = registry.meysamaghili.ir
DNS.2 = registry
DNS.3 = localhost
IP.1 = 127.0.0.1
